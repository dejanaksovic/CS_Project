<Window x:Class="GraphVisual.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GraphVisual" xmlns:views="clr-namespace:GraphVisual.Views"
        mc:Ignorable="d"
        Title="GraphVis" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <views:Graph_control DataContext="{Binding CurrentViewModel}"/>
        <views:Graph_space DataContext="{Binding GraphSpace}" Grid.Column="1"/>
    </Grid>
</Window>
